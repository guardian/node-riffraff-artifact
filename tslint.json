{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "import-name": false,
    "no-console":false,
    "variable-name": [
      true,
      "allow-pascal-case",
      "allow-trailing-underscore"
    ],
    "no-default-export": {
      "severity": "warning"
    },
    "prettier": true
  },
  "rulesDirectory": [],
  "linterOptions": {}
}